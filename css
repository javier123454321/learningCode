  right:20;
  animation-delay: 1s;
  animation-name: spin2;

  opacity: 1;
}

@keyframes spin1 {
    0% {
        transform: rotate(-80deg) scale(1.5);
    }
  79%{

        filter: drop-shadow(0);
  }
  80% {
        filter: drop-shadow(10px 10px 4px grey);
  }
    100% {
        transform:rotate(375deg);
        filter: drop-shadow(3px 1px 4px black);
    }
}
